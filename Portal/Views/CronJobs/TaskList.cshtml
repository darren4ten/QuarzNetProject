@model IEnumerable<Portal.Models.JobRecord>
@{
    Layout = "~/Views/Shared/_MainLayout.cshtml";
    ViewBag.Title = "任务列表";

}

<table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>Job显示名称</th>
            <th>Job名称</th>
            <th>重复模式</th>
            <th>是否可用</th>
            <th>运行状态</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td><a href="/CronJobs/Edit?id=@item.ID">@item.JobDisplayName</a></td>
                <td>@item.JobName</td>
                <td>@item.RepeatMode</td>
                <td>@item.IsEnabled</td>
                <td>--</td>
            </tr>
        }
    </tbody>
</table>
